<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<UseWindowsForms>True</UseWindowsForms>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>True</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="bin\Debug\net9.0-windows\**" />
    <Compile Remove="Configuration\**" />
    <EmbeddedResource Remove="bin\Debug\net9.0-windows\**" />
    <EmbeddedResource Remove="Configuration\**" />
    <None Remove="bin\Debug\net9.0-windows\**" />
    <None Remove="Configuration\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Class1.cs" />
    <Compile Remove="CouldUpload\DropBoxUploader1.cs" />
    <Compile Remove="FallbackData.Designer.cs" />
    <Compile Remove="Models\Spec.cs" />
    <Compile Remove="Resource1.Designer.cs" />
    <Compile Remove="Services\fileReadServices.cs" />
    <Compile Remove="Tools\OAuth2WebView.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="FallbackData.resx" />
    <EmbeddedResource Remove="Resource1.resx" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Resources\Proxies.txt" />
    <None Remove="Resources\user_agents.txt" />
    <None Remove="user_agents.txt" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\proxies.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\user_agents.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dropbox.Api" Version="7.0.0" />
    <PackageReference Include="HtmlAgilityPack" Version="1.12.2" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\evry1falls.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>evry1falls.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\evry1falls.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>evry1falls.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="bin\Debug\" />
  </ItemGroup>

</Project>
